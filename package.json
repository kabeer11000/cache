{
  "name": "nano-cache",
  "version": "2.2.0",
  "description": "Ultra-small (<5KB) full-featured in-memory cache with TTL, LRU, stale-while-revalidate, async deduplication and zero dependencies",
  "main": "dist/nano-cache.cjs.js",
  "module": "dist/nano-cache.esm.js",
  "types": "dist/nano-cache.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/nano-cache.esm.js",
      "require": "./dist/nano-cache.cjs.js",
      "types": "./dist/nano-cache.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "test": "node test.js"
  },
  "keywords": [
    "cache",
    "in-memory",
    "ttl",
    "lru",
    "memoize",
    "serverless",
    "tiny",
    "fast",
    "stale-while-revalidate"
  ],
  "author": "Seyed Ali Mohammadiyeh <maxbasecode@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BaseMax/nano-cache.git"
  },
  "bugs": {
    "url": "https://github.com/BaseMax/nano-cache/issues"
  },
  "homepage": "https://github.com/BaseMax/nano-cache#readme",
  "devDependencies": {
    "rollup": "^4.24.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}